<UserControl x:Class="PodCatcherNet9.SpeedProj" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignHeight="22" d:DesignWidth="90" x:Name="me3">
	<Grid Width="90" >
		<!--
		<DataGridTemplateColumn     Header="Mb/min  min  eta" >...
		-->
		<TextBlock Text="{Binding ElementName=me3, Path=SpeedMbMin,   FallbackValue='  2.1'}"		VerticalAlignment="Center" HorizontalAlignment="Left"   Foreground="#00f" />
		<TextBlock Text="{Binding ElementName=me3, Path=MinutsLeft, FallbackValue='2.7  '}"		VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#090" />
		<TextBlock Text="{Binding ElementName=me3, Path=CurrentETA,   FallbackValue='22.55 '}"	VerticalAlignment="Center" HorizontalAlignment="Right"  Foreground="#800" />

	</Grid>
</UserControl>
